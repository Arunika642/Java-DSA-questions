import java.util.Scanner;
class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    System.out.println("Enter two numbers");
    int a = sc.nextInt();
    int b = sc.nextInt();
    int sum = sum(a,b);
    System.out.println(sum);
  }
  

   static int sum(int a,int b) 
  {
    int s = 0;
    s = a + b;
    return s ;
    
  }
}
