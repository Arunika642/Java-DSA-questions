import java.util.Scanner;

public class Main {
public static void main(String[] args) {
  Scanner in = new Scanner(System.in);
  int s1=0,s2=0,s3=0;
  while(true)
    { 
        int num = in.nextInt();
        if(num==0)
        {
          break;
        }
       if(num<0)
      {
        s1 = s1+ num;
      }
      if(num>=0)
      {
        if(num%2==0)
        {
          s2=s2+num;
        }
        if((num%2)!=0)
        {
          s3 = s3 +num;
        }
        }
      }
   System.out.println("The sum of negative numbers is " +s1);
   System.out.println("The sum of positive even numbers is " +s2);
   System.out.println("The sum of positive odd numbers is " +s3);
}
}
