class Main {
  public static void main(String[] args) {
    Scanner in = new Scanner(System.in);
    int variable =0;
    int sum =0;
    System.out.println("Enter the number and get the sum ");
    while(true)
      {
        int n = in.nextInt();
        if(n==variable)
        {
         break; 
        } 
         if(n!=0)
         {
           sum = sum + n;
         }
      }
    System.out.println(sum);
  }
}
    
