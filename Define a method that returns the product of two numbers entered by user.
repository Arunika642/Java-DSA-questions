import java.util.Scanner;
class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    System.out.println("Enter two numbers");
    int a = sc.nextInt();
    int b = sc.nextInt();
    int prod = prod(a,b);
    System.out.println(prod);
  }
  

   static int prod(int a,int b) 
  {
    int p = 1;
    p = a * b;
    return p ;
    
  }
}
