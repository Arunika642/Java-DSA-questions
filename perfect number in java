import java.util.Scanner;

public class Main {
public static void main(String[] args) {
  Scanner in = new Scanner(System.in);
  int num = in.nextInt();
  int prod = 0;
  for(int i=1;i<num;i++)
    {
      if(num%num==1 || num %i==0)
      {
        prod = prod + i;
      }
    }
  if(prod == num)
  {
    System.out.println(num+ " is a perfect number");
  }
  else
  {
    System.out.println(num+ " is not a perfect number");
  }
}
}
