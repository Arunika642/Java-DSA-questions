import java.util.Scanner;
class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    int n = sc.nextInt();
    prime(n);
    
    
  }
  static void prime(int a)
  {
    for(int i=2;i<a;i++)
      {
        if(a%1==0 && a%i!=0 && a%a==0)
          System.out.println(a+ "is prime");
        else
          System.out.println(a+ "is not prime");
        break;
      }
  }
}
