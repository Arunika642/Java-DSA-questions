import java.util.Scanner;
class Main {
  public static void main(String[] args) {
    Scanner in = new Scanner(System.in);
    float P = in.nextFloat();
    float R = in.nextFloat();
    float T = in.nextFloat();
    float SI; 
    SI = (P*R*T)/100;
    System.out.println("The Simple Interest is " +SI);
  }
}
