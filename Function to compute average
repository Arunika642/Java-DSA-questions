import java.util.Scanner;
class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    int s=0;
    for(int i=1;i<=3;i++)
      {
        int num = sc.nextInt();
        s = s+num;
      }
    double average = Avg(s);
    System.out.println(average);
  }
  static double Avg(int n){
    double avg;
    avg = n/3;
    return avg;  
  }
}
