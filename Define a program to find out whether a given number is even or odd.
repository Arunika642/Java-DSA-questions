import java.util.Scanner;
class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    int a = sc.nextInt();
    boolean isCheck = isCheck(a);
    System.out.println(isCheck);
  }
  

   static boolean isCheck(int z) 
  {
    if(z%2==0)
      return true;
    else
      return false;
  }
}
